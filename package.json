{
  "name": "home-page",
  "version": "1.0.0",
  "description": "A Home page for my personal portofolio",
  "main": "index.js",
  "scripts": {
    "start": "php -S localhost:80",
    "build": "npm run webpack && npm run babel && npm run compress",


    "webpack": "webpack --config webpack.config.js",
    "babel": "babel --presets es2015 assets/build/index.min.js -o assets/build/index.min.js",
    "compress": "minify assets/build -d assets/build"
  },
  "author": "Guilherme Rossato",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-minify": "^0.5.0",
    "babel-preset-es2015": "^6.24.1",
    "webpack": "^4.27.1",
    "webpack-cli": "^3.1.2"
  }
}
